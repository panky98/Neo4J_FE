ojekat.zaposleniNaProjektu.length!==0 ?\r\n                <button onClick={()=>setPrikaziZaposlene(!prikaziZaposlene)}>{dugmeZaposleni}</button> :\r\n                    <p>Trenutno nema nijednog zaposlenog na projektu</p>\r\n            }\r\n            {prikaziZaposlene &&\r\n                <div>\r\n                    {projekat.zaposleniNaProjektu.map(zap=>{\r\n                        return(\r\n                            <p>{zap.ime} {zap.prezime}</p>\r\n                        )\r\n                    })}\r\n                </div>\r\n            }\r\n            <button onClick={()=>obrisiProjekat()}>Obrisi projekat</button>\r\n        </div>)\r\n\r\n}\r\n\r\nexport default Projekat\r\n","E:\\IV godina RI\\Napredne baze podataka\\Neo4J_FE\\FEReact\\FE_REACT\\src\\components\\NapraviProjekat.js",[],"E:\\IV godina RI\\Napredne baze podataka\\Neo4J_FE\\FEReact\\FE_REACT\\src\\services\\useFetch.js",[],"E:\\IV godina RI\\Napredne baze podataka\\Neo4J_FE\\FEReact\\FE_REACT\\src\\components\\Firme.js",[],"E:\\IV godina RI\\Napredne baze podataka\\Neo4J_FE\\FEReact\\FE_REACT\\src\\components\\Firma.js",["104","105","106","107"],"import React, {useState} from 'react'\r\nimport Spinner from './Spinner';\r\n\r\n\r\nfunction Firma({firma}) {\r\n\r\n    const [zaposleniFirme, setZaposleneFirme]=useState([]);\r\n    const [showSpinner,setShowSpinner]=useState(false);\r\n\r\n    return(\r\n        <div>\r\n              {showSpinner && <Spinner/>}\r\n              <h1>{firma.naziv} <a href=\"#\"><span className=\"material-icons\" style={{\"color\":\"red\"}} onClick={async ()=>await ObrisiFirmu()}>delete</span></a></h1>  \r\n              <label>PIB: {firma.pib}</label><br/>\r\n              <label>Adresa: {firma.adresa}</label><br/>\r\n              <select onChange={async (event)=>await onChange(event)}>\r\n                  <option value={0}>Izaberi opciju</option>\r\n                  <option value={1}>Svi zaposleni ikad</option>\r\n                  <option value={2}>Trenutno zaposleni</option>\r\n              </select>\r\n              <ul>\r\n              {zaposleniFirme!=[] && zaposleniFirme.map(zaposleni=>{\r\n                  return <li key={zaposleni[\"id\"]}>{zaposleni[\"ime\"] + \" \"+ zaposleni[\"prezime\"]}</li>\r\n              })}\r\n              </ul>\r\n        </div>\r\n    )\r\n\r\n    async function onChange(event){\r\n       if(event.target.value==0){\r\n           setZaposleneFirme([]);\r\n           return;\r\n       }\r\n\r\n       setShowSpinner(true);\r\n       if(event.target.value==1){\r\n       await fetch(\"https://localhost:44392/Zaposleni/vratiSveZaposlenePrekoIdFirme/\"+firma.id,{\r\n           method:\"GET\"\r\n       }).then(p=>{\r\n           if(p.ok){\r\n               p.json().then(data=>{\r\n                   setZaposleneFirme(data);\r\n               })\r\n           }\r\n       }).catch(ex=>{\r\n           console.log(ex);\r\n       });\r\n    }\r\n    else{\r\n        await fetch(\"https://localhost:44392/Zaposleni/vratiSveZaposlenePrekoIdFirmeTrenutnoZaposleni/\"+firma.id,{\r\n            method:\"GET\"\r\n        }).then(p=>{\r\n            if(p.ok){\r\n                p.json().then(data=>{\r\n                    setZaposleneFirme(data);\r\n                })\r\n            }\r\n        }).catch(ex=>{\r\n            console.log(ex);\r\n        });\r\n    }\r\n       setShowSpinner(false);\r\n    }\r\n\r\n    async function ObrisiFirmu(){\r\n        setShowSpinner(true);\r\n        await fetch(\"https://localhost:44392/Firma/obrisiFirmu/\"+firma.id,{\r\n            method:\"DELETE\"\r\n        }).then(p=>{\r\n            if(p.ok){\r\n                window.location.reload(false);\r\n            }\r\n        }).catch(ex=>{\r\n            console.log(ex);\r\n        });\r\n        setShowSpinner(false);\r\n    }\r\n}\r\n\r\nexport default Firma;","E:\\IV godina RI\\Napredne baze podataka\\Neo4J_FE\\FEReact\\FE_REACT\\src\\components\\CreateFirma.js",["108","109","110"],"import React, {u